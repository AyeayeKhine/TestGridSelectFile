            MemberIdList += "," + selectedMemberList[i].id;
        }
        var params = {
            filter: '["ID", "=", "' + MemberIdList + '"]'
        };
        var url = "/Home/ChangeSafeBox";
        loadData(url, "json", "POST", "application/x-www-form-urlencoded", params)
            .then(function (result) {
                $(gridIDbySafeBox).dxDataGrid("instance").refresh();
                $(gridID).dxDataGrid("instance").refresh();
            });
    }

