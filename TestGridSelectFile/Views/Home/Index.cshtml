            MemberIdList = selectedMemberList[0].id;
        }
        for (var i = 1; i < selectedMemberList.length; i++) {

            MemberIdList += "," + selectedMemberList[i].id;
        }
        var params = {
            filter: '["ID", "=", "' + MemberIdList + '"]'
        };
        var url = "/Home/ChangeInHand";
        loadData(url, "json", "POST", "application/x-www-form-urlencoded", params)
            .then(function (result) {
                $(gridIDbySafeBox).dxDataGrid("instance").refresh();
                $(gridID).dxDataGrid("instance").refresh();
            });
        
    }

    function GetDataSource(url, parentPropoertyName, parentID, extrafilter) {
        if (isNotEmpty(parentID) && (parentID)) {
            var url = url + "ByFilter?SelectedTextProperty=" + parentPropoertyName + "&ParentID=" + parentID + (isNotEmpty(extrafilter) ? extrafilter : "");
        } else if (isNotEmpty(extrafilter)) {
            var url = url + "ByFilter?" + extrafilter;
        }

        var dataSource = new DevExpress.data.CustomStore({
            loadMode: 'raw',
            cacheRawData: false,
            key: 'id',
            load: function (loadOptions) {
                var d = $.Deferred(),
                    params = { prefix: loadOptions.searchValue };

                loadData(url, "json", "GET", "application/x-www-form-urlencoded", params)
                    .then(function (result) {
                        d.resolve(result.data == "" ? result.data : result.data.value);

                    });
                return d.promise();
            },
            byKey: function (key) {
                var d = $.Deferred(),
                    params = { prefix: key };
                loadData(url, "json", "GET", "application/x-www-form-urlencoded", params)
                    .then(function (result) {
                        d.resolve(result.data == "" ? result.data : result.data.value);
                    });
                return d.promise();
            }

        });
        return dataSource;
    };

    function GetItemOrderStatusList() {
        var GetItemOrderStatusList = [

            { Value: "0", Text: "SafeBox" },
            { Value: "1", Text: "Store" },
            { Value: "2", Text: "In hand" }
        ];
        return GetItemOrderStatusList;
    };

